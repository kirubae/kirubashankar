{
  "manifest_version": 3,
  "name": "Eleos B2B",
  "version": "1.0",
  "description": "LinkedIn contact enrichment for B2B sales",
  "permissions": [
    "storage",
    "clipboardWrite"
  ],
  "host_permissions": [
    "https://www.linkedin.com/*",
    "https://*.workers.dev/*",
    "http://localhost/*",
    "http://127.0.0.1:8787/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://*.linkedin.com/*"],
      "js": ["config.js", "content.js"]
    },
    {
      "matches": ["https://mail.google.com/*"],
      "js": ["gmail-content.js"],
      "css": ["gmail-styles.css"],
      "run_at": "document_end"
    }
  ],
  "options_page": "options.html",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}